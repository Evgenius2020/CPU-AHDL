include "lpm_rom.inc";

subdesign rom
(
	addr[0..7] : input;
	out[0..7] : output;
)
variable
	mem: lpm_rom with (
		LPM_WIDTH = 8, 
		LPM_WIDTHAD = 8,
		LPM_ADDRESS_CONTROL = "UNREGISTERED",
		LPM_OUTDATA = "UNREGISTERED",
		LPM_FILE = "fib.mif");
begin
	mem.address[] = addr[];
	out[] = mem.q[];
end;
